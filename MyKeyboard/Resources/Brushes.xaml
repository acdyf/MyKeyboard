<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="DefaultBackgroundBrush" Color="{DynamicResource DefaultBackgroundColor}"/>
    <SolidColorBrush x:Key="DefaultBorderBrush" Color="{DynamicResource DefaultBorderColor}"/>
    <SolidColorBrush x:Key="DefaultButtonExBackgroundBrush" Color="{DynamicResource DefaultButtonExBackgroundColor}"/>
    <SolidColorBrush x:Key="DefaultButtonExForegroundBrush" Color="{DynamicResource DefaultButtonExForegroundColor}"/>
    <SolidColorBrush x:Key="DefaultButtonExMouseOverBackgroundBrush" Color="{DynamicResource DefaultButtonExMouseOverBackgroundColor}"/>
    <SolidColorBrush x:Key="DefaultButtonExMouseOverForegroundBrush" Color="{DynamicResource DefaultButtonExMouseOverForegroundColor}"/>
    <SolidColorBrush x:Key="DefaultButtonExPressedBackgroundBrush" Color="{DynamicResource DefaultButtonExPressedBackgroundColor}"/>
    <SolidColorBrush x:Key="DefaultButtonExPressedForegroundBrush" Color="{DynamicResource DefaultButtonExPressedForegroundColor}"/>
    <SolidColorBrush x:Key="DefaultButtonExLightBackgroundBrush" Color="{DynamicResource DefaultButtonExLightBackgroundColor}"/>
    <SolidColorBrush x:Key="DefaultButtonExLightForegroundBrush" Color="{DynamicResource DefaultButtonExLightForegroundColor}"/>
</ResourceDictionary>